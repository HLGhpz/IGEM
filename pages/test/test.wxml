<!-- 按钮1 -->
<view class="box_switch">
    <view class="box_switch_L">
        <view class="box_switch_L_U">
            <view class="switch_text">Add O2</view>
            <view class="switch_button"></view>
        </view>
        <view class="box_switch_L_D">
            <view class="log_text2">上次发送时间</view>
            <view class="log_time2">{{date1}}</view>
        </view>

    </view>


    <view class="box_switch_C" wx:if="{{switch1Checked==false}}">
        <switch checked="{{switch1Checked}}" bindchange="switch1Change"></switch>
    </view>
    <view class="box_switch_C" wx:if="{{switch1Checked==true}}">
        <switch disabled="true" checked="{{switch1Checked}}" bindchange="switch1Change"></switch>
    </view>

    <view class="box_switch_R">
        <image class="box_image" src="../../images/o2.png"></image>
    </view>
</view>

